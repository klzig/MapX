<?xml version="1.0" encoding="utf-8" ?>
<ViewsBase:NotificationsBasePage xmlns="http://xamarin.com/schemas/2014/forms"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:ViewsBase="clr-namespace:CTForms.ViewsBase;assembly=CTForms"
    xmlns:vm="clr-namespace:CTForms.ViewModels"
    x:Class="CTForms.Views.NotificationsAlertPage"
    Title="{Binding Title}">
    
    <ContentPage.BindingContext>
        <vm:NotificationsViewModel />
    </ContentPage.BindingContext>
    
    <ContentPage.Content>
        <StackLayout>
            <Label Text="NotificationsAlertPage"
                VerticalOptions="CenterAndExpand" 
                HorizontalOptions="CenterAndExpand" />
        </StackLayout>
    </ContentPage.Content>
</ViewsBase:NotificationsBasePage>